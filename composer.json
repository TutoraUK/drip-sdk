{
    "name": "snscripts/drip-sdk",
    "description": "Unofficial SDK for Drip Email Marketing Software",
    "keywords": ["Drip", "SDK", "Email", "Marketing"],
    "homepage": "https://github.com/mikebarlow/drip-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "6.3.*",
        "cartalyst/collections": "1.1.*",
        "snscripts/getset": "1.0.*",
        "snscripts/result": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.5",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Snscripts\\Drip\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snscripts\\Drip\\Tests\\": "tests"
        }
    }
}
